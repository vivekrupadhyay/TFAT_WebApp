@{
    ViewBag.Title = "frmTransfer";
}
<link href="~/Content/jqTree/jqtree.css" rel="stylesheet" type="text/css" />
<link href="~/Content/jquery-ui-1.10.4.custom.css/cupertino/jquery-ui-1.10.4.custom.css" rel="stylesheet" />
<link href="~/Content/jquery-ui-1.10.4.custom.css/cupertino/jquery-ui-1.10.4.custom.min.css" rel="stylesheet" />
<script src="~/Scripts/App/AppJQueryUI.js"></script>
<script type="text/javascript" src="~/Scripts/jqTree/tree.jquery.js"></script>
<script src="~/Scripts/i18n/grid.locale-en.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.jqGrid.min.js" type="text/javascript"></script>
<script type="text/javascript" src="~/Scripts/App/JQMenu.js"></script>

<script src="~/Scripts/App/frmTransfer.js"></script>
@using (Html.BeginForm("Index", "frmTransfer", FormMethod.Post, new { enctype = "Multipart/formdata" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <table style="border-top: 1px solid; border-bottom: 1px solid; border-left: 1px solid; border-right: 1px solid;  border-color: #a5a8ab;">
        <tr>
            <td>Goods Outword Covering(GOC)
                <table style="border-top: 1px solid; border-bottom: 1px solid; border-left: 1px solid; border-right: 1px solid;  border-color: #a5a8ab;">
                    <tr>
                        <td style="border-right: 1px solid; width: 35%; padding-left: 5px;">
                            <table>
                                <tr>
                                    <td>

                                        DocDate:

                                    </td>
                                    <td>
                                        @Html.TextBox("txt WoNo", null, new { style = "width:235px; font-size:9pt;" })
                                    </td>

                                </tr>
                                <tr>
                                    <td>
                                        LastSNo:
                                    </td>
                                    <td>@Html.Partial("_User", "")</td>
                                </tr>
                                <tr>
                                    <td>
                                        SerialNo:
                                    </td>
                                    <td>
                                        @Html.Partial("_User", "")
                                    </td>

                                </tr>
                                <tr>
                                    <td colspan="2">
                                        <hr />

                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        Source:
                                    </td>
                                    <td>
                                        @Html.DropDownList("Address", new SelectList(string.Empty, "Value", "Text"), "----select---", new { id = "AddressNo.", style = "width:240px; height:20px; font-size:9pt;" })
                                    </td>
                                </tr>
                            </table>
                        </td>

                        <td>
                            <table>
                                <tr>
                                    <td style="padding-left: 5px; ">
                                        Transfer NoteNo:
                                    </td>
                                    <td>
                                        @Html.TextBox("txt WoNo", null, new { style = "width:235px; font-size:9pt;" })
                                    </td>
                                    <td style="padding-left: 5px; ">
                                        Date:
                                    </td>
                                    <td>
                                        @Html.TextBox("txt WoNo", null, new { style = "width:235px; font-size:9pt;" })
                                    </td>
                                </tr>
                                <tr>
                                    <td style="padding-left: 5px; ">
                                        From Store:
                                    </td>
                                    <td>
                                        @Html.Partial("_User", "")
                                    </td>
                                </tr>
                                <tr>
                                    <td style="padding-left: 5px; ">
                                        To Store:
                                    </td>
                                    <td>
                                        @Html.Partial("_User", "")
                                    </td>
                                </tr>
                                <tr>
                                    <td style="padding-left: 5px; ">
                                        Work OrderNo.:
                                    </td>
                                    <td>
                                        @Html.TextBox("txt WoNo", null, new { style = "width:235px; font-size:9pt;" })
                                    </td>
                                    <td style="padding-left: 5px; ">
                                        Stage:
                                    </td>
                                    <td style="padding-left: 5px; ">
                                        @Html.DropDownList("Address", new SelectList(string.Empty, "Value", "Text"), "----select---", new { id = "AddressNo.", style = "width:240px; height:20px; font-size:9pt;" })
                                    </td>
                                </tr>
                                <tr>
                                    <td></td>
                                    <td></td>
                                    <td colspan="2" align="right">
                                        <input type="checkbox" />Return Goods?
                                    </td>
                                </tr>
                            </table>

                        </td>
                    </tr>
                </table>
                <table id="list" style="width: 100%"></table>
                <div id="pager"></div>
            </td>
        </tr>
    </table>

}
